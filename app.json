{
  "expo": {
    "name": "CarLife",
    "slug": "CarLife",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/CarlifeIcon.ico",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/CarLifeAdaptive.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/img/CarlifeIcon.png"
    },
    "description": "Resolve DW",
    "githubUrl": "https://github.com/ksromero92/CarLife"
  }
}
